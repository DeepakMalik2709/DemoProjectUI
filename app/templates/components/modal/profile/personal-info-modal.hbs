{{#modal/custom-modal title="Personal Information" modal-id="personal-info" item=data save="update"}}
    <div class="form-group has-feedback has-success">
      <label class="control-label col-sm-3" for="inputdob">DOB</label>
      <div class="col-sm-8">
        {{input type="text" value=data.dob class="form-control" id="inputdob"}}
      </div>
    </div>
    <div class="form-group has-feedback has-success">
      <label class="control-label col-sm-3" for="inputHomeTown">Hometown</label>
      <div class="col-sm-8">
        {{input type="text" value=data.homeTown class="form-control" id="inputHomeTown"}}
      </div>
    </div>
{{/modal/custom-modal}}